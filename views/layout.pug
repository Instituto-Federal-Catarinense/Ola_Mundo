doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css')
  body
    block content
      .container
        .row
          .col-md-8.mx-auto
            .carousel.slide(data-ride='carousel', id='carouselExampleSlidesOnly')
              .carousel-inner
                .carousel-item.active
                  img.d-block.w-100(src='data:image/jpeg;base64,', alt='Primeiro slide')
                .carousel-item
                  img.d-block.w-100(src='https://cdn.espn.com.br/image/wide/622_c416f2fa-d5ab-35a9-9b1c-e452994de32c.jpg', alt='Segundo slide')
                .carousel-item
                  img.d-block.w-100(src='https://www.campeoesdofutebol.com.br/poster/inter_campeao_sulamericana_2008.jpg', alt='Terceiro slide')
              a.carousel-control-prev(href='#carouselExampleSlidesOnly', role='button', data-slide='prev')
                span.carousel-control-prev-icon(aria-hidden='true')
                span.sr-only Anterior
              a.carousel-control-next(href='#carouselExampleSlidesOnly', role='button', data-slide='next')
                span.carousel-control-next-icon(aria-hidden='true')
                span.sr-only Próximo

  script(src='https://code.jquery.com/jquery-3.5.1.slim.min.js')
  script(src='https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.4/dist/umd/popper.min.js')
  script(src='https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js')


