extends layout

block content
  h1= title
  p Bem-vindo à página #{title}

  // Conteúdo Principal
  main.main-content
    .container.text-center
      // Primeira Linha de Quadrados
      .row.row-cols-1.row-cols-md-2.row-cols-lg-3.g-4
        // Quadrado - Funk
        .col.mb-4
          a(href="/musicas/funk")
            .border.text-center.p-4
              img.img-fluid(src="https://meupiaui.com/wp-content/uploads/2024/01/Funk-e-Cultura-Como-Entender-Esse-Fenomeno-1-1.webp" alt="funk")
              h5.text-dark melhor estilo!
              p.text-muted Para aquela festinha mais para cima!
        // Quadrado - Pagode
        .col.mb-4
          a(href="/musicas/pagode")
            .border.text-center.p-4
              img.img-fluid(src="https://static.wixstatic.com/media/8bb472_defe4a63b17b4e95b05786d887e0504f~mv2.jpg/v1/fill/w_490,h_316,al_c,q_80,usm_0.66_1.00_0.01,enc_auto/Pagode.jpg" alt="pagode")
              h5.text-dark Domingueira
              p.text-muted Para aquele domingão de churrasco com os amigos
        // Quadrado - Poesia
        .col.mb-4
          a(href="/musicas/poesia")
            .border.text-center.p-4
              img.img-fluid(src="https://miro.medium.com/v2/resize:fit:1400/1*_eRiEgigChk6SP9YQtawXQ.jpeg" alt="poesia")
              h5.text-dark para quem precisa aquecer o coracão 
              p.text-muted Só para os românticos
        // Quadrado - Pop
        .col.mb-4
          a(href="/musicas/pop")
            .border.text-center.p-4
              img.img-fluid(src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRTPXiCsLu5zFm_jtoyrZy1obKfD6lSFxIiGg&s" alt="pop")
              h5.text-dark só as de fora né
              p.text-muted Aquele mais internacional 
        // Quadrado - Romântica
        .col.mb-4
          a(href="/musicas/romantica")
            .border.text-center.p-4
              img.img-fluid(src="data:image/jpeg;base64," alt="romantica")
              h5.text-dark Essa é de arrastar no asfalto 
              p.text-muted Só pra quem já viveu 
        // Quadrado - Trapp
        .col.mb-4
          a(href="/musicas/Trapp")
            .border.text-center.p-4
              img.img-fluid(src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRFw12jdDIhEjSqxvV-h-KErv88pAM3PWb6dQ&s" alt="Trapp")
              h5.text-dark Só os novinho 
              p.text-muted Matuê muda vidas!

  // Rodapé (igual ao layout fornecido)
  script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js")
